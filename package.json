{
  "name": "quad-blog",
  "version": "1.0.0",
  "description": "Isomorphic flux and react blog",
  "main": "./index.js",
  "private": true,
  "scripts": {
    "clean": "rm -rf public build",
    "build": "npm run build:client && npm run build:server",
    "build:server": "webpack --config webpack.production.js --bail",
    "build:client": "webpack --config webpack.build.js --bail -p",
    "db:create": "create_database",
    "db:drop": "drop_database",
    "db:migrate": "sequelize db:migrate",
    "db:refresh": "npm run db:drop && npm run db:create && npm run db:migrate",
    "dev": "npm run dev:server & npm run dev:client",
    "dev:client": "webpack-dev-server --hot --inline --progress",
    "dev:server": "npm run dev:server:webpack & npm run dev:server:nodemon",
    "dev:server:webpack": "webpack --config webpack.server.js --watch",
    "dev:server:nodemon": "nodemon -e js,json -w ./build/ ./build/server.js",
    "predev:server:nodemon": "mkdirp build && touch ./build/server.js",
    "lint": "standard"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/cesarandreu/quad-blog.git"
  },
  "keywords": [
    "web",
    "application"
  ],
  "standard": {
    "parser": "babel-eslint"
  },
  "author": "Cesar Andreu <cesarandreu@gmail.com> (https://github.com/cesarandreu)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/cesarandreu/quad-blog/issues"
  },
  "homepage": "https://github.com/cesarandreu/quad-blog#readme",
  "dependencies": {
    "autoprefixer-core": "^5.2.1",
    "babel": "^5.6.14",
    "babel-core": "^5.6.15",
    "babel-loader": "^5.3.1",
    "babel-runtime": "^5.6.15",
    "classnames": "^2.1.3",
    "css-loader": "^0.15.1",
    "debug": "^2.2.0",
    "extract-text-webpack-plugin": "^0.8.1",
    "file-loader": "^0.8.4",
    "fluxible": "^0.4.12",
    "fluxible-immutable-utils": "^0.3.2",
    "fluxible-router": "^0.1.9",
    "github-download": "^0.3.0",
    "immutable": "^3.7.4",
    "inflection": "^1.7.1",
    "invariant": "^2.1.0",
    "keymirror": "^0.1.1",
    "koa": "^0.21.0",
    "koa-compose": "^2.3.0",
    "koa-logger": "^1.2.2",
    "koa-qs": "^2.0.0",
    "koa-response-time": "^1.0.2",
    "koa-router": "^5.1.2",
    "koa-static": "^1.4.9",
    "less": "^2.5.1",
    "less-loader": "^2.2.0",
    "lodash": "^3.10.0",
    "marked": "^0.3.3",
    "mkdirp": "^0.5.1",
    "moment": "^2.10.3",
    "node-fetch": "^1.3.0",
    "node-libs-browser": "^0.5.2",
    "normalize.css": "^3.0.3",
